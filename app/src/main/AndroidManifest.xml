<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="id.ac.polbeng.amandaagungpermata.p8pro22">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.P8Pro22"
        android:usesCleartextTraffic="true"
        tools:targetApi="n">
        <activity
            android:name=".activities.DetailJasa"
            android:exported="false"
            android:label="Detail Jasa"
            android:parentActivityName=".activities.MainActivity"
            tools:ignore="Instantiatable" />
        <activity
            android:name=".activities.EditProfile"
            android:exported="false"
            android:label="Ubah Profile"
            android:parentActivityName=".activities.MainActivity"
            tools:ignore="Instantiatable" />
        <activity
            android:name=".activities.Register"
            android:exported="false"
            android:label="Daftar Akun"
            android:parentActivityName=".activities.Login"
            tools:ignore="Instantiatable" />
        <activity
            android:name=".activities.Login"
            android:exported="false"
            android:theme="@style/Theme.P8Pro22.NoActionBar"
            tools:ignore="Instantiatable" />
        <activity
            android:name=".activities.SplashScreenAcktivity"
            android:exported="true"
            android:theme="@style/Theme.P8Pro22.NoActionBar"
            tools:ignore="Instantiatable">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.MainActivity"
            android:exported="true"
            android:theme="@style/Theme.P8Pro22.NoActionBar"
            tools:ignore="Instantiatable" />
    </application>

</manifest>